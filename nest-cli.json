{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/toy-nestjs/src",
  "monorepo": true,
  "root": "apps/toy-nestjs",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/toy-nestjs/tsconfig.app.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "entity": {
      "type": "library",
      "root": "libs/entity",
      "entryFile": "index",
      "sourceRoot": "libs/entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entity/tsconfig.lib.json"
      }
    },
    "common-config": {
      "type": "library",
      "root": "libs/common-config",
      "entryFile": "index",
      "sourceRoot": "libs/common-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common-config/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    }
  }
}